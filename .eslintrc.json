{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "@papermana/eslint-config-personal",
    "@papermana/eslint-config-personal/react"
  ],
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-invalid-this": 0
  }
}
